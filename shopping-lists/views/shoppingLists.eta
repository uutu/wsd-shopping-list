<h1>Shopping lists</h1>

<h2>Add a new shopping list</h2>

<form method="POST" action="/lists">
    Name: <input type="text" name="name" />
    <input type="submit" value="Add a new list" />
</form>

<h2>Active shopping lists</h2>

<ul>
<% it.lists.forEach((task) => { %>
  <li><%= task.name %></li>
<% }); %>
</ul>